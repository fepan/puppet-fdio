unix {
  cli-listen localhost:<%=@fdio_port%>
  nodaemon
  log /tmp/vpp.log
  full-coredump
}

dpdk {
  <% @fdio_dpdk_pci_devs.each do |n| -%>
  dev <%=n-%> <% if system("lspci -s #{n} | grep VIC") -%> {vlan-strip-offload off}<%end-%>
  <% end -%>
  uio-driver <%=@dpdk_pmd_type%>
}

api-trace {
  on
}

api-segment {
  gid vpp
}
