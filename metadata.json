{
  "name": "puppet-fdio",
  "version": "0.0.1",
  "author": "Feng Pan <fpan@redhat.com>",
  "summary": "Puppet module for fdio projects",
  "license": "Apache-2.0",
  "source": "https://git.fd.io/puppet-fdio",
